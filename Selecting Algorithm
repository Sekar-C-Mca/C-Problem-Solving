// Online C++ compiler to run C++ program online
#include <bits/stdc++.h>
using namespace std;

int main() {
    int size;
    cin>>size;
    int arr[size];//array with size
    //input
    for(int i=0;i<size;i++){
        cin>>arr[i];
    }
    //logic
    //Pointing the element the outer element 
    int index=0;//elemnt for compairing first elemnt is smaller or not
    for(int i=0;i<=size-2;i++){
        index=i;
        for(int j=i+1;j<size;j++){
            if(arr[j]<arr[index]){
                index=j;
            }
        }
        if(index!=i){
            swap(arr[index],arr[i]);
        }
    }
    
    
    //output
    for(int i=0;i<size;i++){
        cout<<arr[i]<<" ";
    }
}
